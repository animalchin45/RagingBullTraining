<% include partials/header %>

<section class="test">
	<div class="title">
		<h2 class="u-margin-bottom">Testimonials</h2>
	</div>
	<div class="test__content">
		<ul>
			<% reviews.forEach((client) => { %>
				<li>
					<div>
						<h2><%= client.name %></h2>
						<p><%= client.review %></p>
					</div>
				</li>
			<% }) %>
		</ul>
	</div>
	<div>
		<button class="button">More Reviews on Google</button>
	</div>
</section>

<% include partials/footer %>